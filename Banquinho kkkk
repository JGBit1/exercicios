cinquentareais = 0
vintereais = 0
dezreais = 0
umreal = 0
valor=int(input('Qual o valor?'))
def saque(valor,cinquentareais,vintereais,dezreais,umreal):
    if valor >= 50:
        cinquentareais = valor // 50
        resto = valor % 50
        saque(resto)
    elif valor >= 20:
        vintereais = valor // 20
        resto = valor % 20
        saque(resto)
    elif valor >=10:
        dezreais = valor // 10
        resto = valor % 10
        saque(resto)
    elif valor >=1:
        umreal = valor
    return valor,cinquentareais,vintereais,dezreais,umreal

print('banco')
saque(valor,cinquentareais,vintereais,dezreais,umreal)
print('VocÃª vai receber {} notas de 50,{} notas de 20,{} notas de 10,e {} moedas de 1'.format(cinquentareais,vintereais,dezreais,umreal))
