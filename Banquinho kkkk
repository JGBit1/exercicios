
valor=int(input('Qual o valor?'))
def saque(valor):
    cemreais = 0
    cinquentareais = 0
    vintereais = 0
    dezreais = 0
    cincoreais = 0
    umreal = 0

    if valor >=100:
        cemreais = valor // 100
        resto = valor % 100
        saque(resto)

    elif valor >= 50:
        cinquentareais = valor // 50
        resto = valor % 50
        saque(resto)

    elif valor >= 20:
        vintereais = valor // 20
        resto = valor % 20
        saque(resto)

    elif valor >=10:
        dezreais = valor // 10
        resto = valor % 10
        saque(resto)
    elif valor >=5:
        cincoreais= valor // 5
        resto =  valor % 5
        saque(resto)

    elif valor >=1:
        umreal = valor

    return cemreais,cinquentareais,vintereais,dezreais,cincoreais,umreal

print('BANCO')
cemreais,cinquentareais,vintereais,dezreais,cincoreais,umreal = saque(valor)
print('VocÃª vai receber {} notas de 100,{} notas de 50,{} notas de 20, {} notas de 10,{} notas de 5, {} moedas de 1'.format(cemreais,cinquentareais,vintereais,dezreais,cincoreais,umreal))
